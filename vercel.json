{
  "version": 2,
  "builds": [
    {
      "src": "server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "server.js"
    }
  ],
  "env": {
    "OPENAI_API_KEY": {
      "type": "secret",
      "value": "@openai_api_key"
    }
  },
  "projectSettings": {
    "framework": "other",
    "installCommand": "npm install",
    "buildCommand": "npm run build",
    "outputDirectory": null,
    "startCommand": "npm start",
    "rootDirectory": null
  }
}
